<%= render partial:"/shared/error", collection:@errors if @errors%>
<h2>Register</h2>

<%= form_for @user do |f| %>

	<%= f.label :full_name %>
	<%=	f.text_field :full_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit %>

<% end %>